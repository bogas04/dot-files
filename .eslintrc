{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "settings": {
    "ecmascript": 6,
    "jsx": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "strict": 0,
    "quotes": 0,
    "no-unused-vars": 2,
    "camelcase": 1,
    "no-underscore-dangle": 0,
    "space-before-blocks": 1,
    "space-before-function-paren": 1,
    "space-in-parens": 0,
    "space-infix-ops": 1,
    "space-unary-ops": 0,
    "spaced-comment": 2,
    "eqeqeq": 2,
    "no-redeclare": 2
  }
}
